# Consciousness Zero Command Center Environment Configuration
# Copy this file to .env and configure your API keys

# IO Intelligence API Configuration
# Get your API key from https://api.intelligence.io.solutions
IO_API_KEY=your_io_intelligence_api_key_here

# Hugging Face Configuration  
# Get your token from https://huggingface.co/settings/tokens
HF_TOKEN=your_huggingface_token_here
OPENAI_API_KEY=your_openai_api_key_here

# Server Configuration
API_HOST=0.0.0.0
API_PORT=8000
GRADIO_HOST=0.0.0.0
GRADIO_PORT=7860

# Model Configuration
DEFAULT_TEMPERATURE=0.7
DEFAULT_MAX_TOKENS=1024
ENABLE_CUDA=true

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=ai_command_center.log

# Security
ENABLE_CORS=true
MAX_CONVERSATION_HISTORY=1000

# Performance
ENABLE_MODEL_CACHING=true
MAX_CONCURRENT_REQUESTS=10
